{
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api", "dest": "/api/index.js" },
    { "src": "/api/cron", "dest": "/api/cron.js" }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://t.me/pingflix_bot",
      "permanent": false
    }
  ]
}
