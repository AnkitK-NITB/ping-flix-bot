{
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/", "dest": "/api/index.js" },
    { "src": "/api", "dest": "/api/index.js" },
    { "src": "/api/cron", "dest": "/api/cron.js" }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 */6 * * *"
    }
  ]
}
